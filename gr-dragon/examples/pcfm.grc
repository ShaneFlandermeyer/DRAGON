options:
  parameters:
    author: Shane Flandermeyer
    category: '[GRC Hier Blocks]'
    cmake_opt: ''
    comment: "Modulates a phase code with the PCFM framework, and\nplots a comparison\
      \ of the phase functions along with the resulting \ncomplex baseband waveform.\n\
      \nNOTE: This flowgraph relies on the PCFM Modulator Hierarchical\nblock, which\
      \ can be used by compiling the flowgraph in \ngrc/PCFM_Modulator.grc then refreshing\
      \ the block list or \nrestarting GRC."
    copyright: ''
    description: ''
    gen_cmake: 'On'
    gen_linking: dynamic
    generate_options: qt_gui
    hier_block_src_path: '.:'
    id: pcfm
    max_nouts: '0'
    output_language: python
    placement: (0,0)
    qt_qss_theme: ''
    realtime_scheduling: ''
    run: 'True'
    run_command: '{python} -u {filename}'
    run_options: prompt
    sizing_mode: fixed
    thread_safe_setters: ''
    title: PCFM
    window_size: ''
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [16, 12.0]
    rotation: 0
    state: enabled

blocks:
- name: bandwidth
  id: variable
  parameters:
    comment: ''
    value: code_len/pulse_width
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [752, 12.0]
    rotation: 0
    state: true
- name: chip_width
  id: variable
  parameters:
    comment: ''
    value: samp_interval*oversampling
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [568, 12.0]
    rotation: 0
    state: true
- name: code_len
  id: variable
  parameters:
    comment: ''
    value: '200'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [384, 12.0]
    rotation: 0
    state: true
- name: oversampling
  id: variable
