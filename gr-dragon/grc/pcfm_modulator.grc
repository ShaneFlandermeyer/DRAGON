    short_id: ''
    type: eng_float
    value: '1'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [192, 12.0]
    rotation: 0
    state: true

connections:
- [blocks_stream_to_vector_0, '0', dragon_difference_xx_0, '0']
- [blocks_vector_to_stream_0, '0', pad_sink_0, '0']
- [dragon_difference_xx_0, '0', dragon_oversample_vector_0, '0']
- [dragon_dti_0, '0', blocks_vector_to_stream_0, '0']
- [dragon_oversample_vector_0, '0', dragon_shaping_filt_0, '0']
- [dragon_shaping_filt_0, '0', dragon_dti_0, '0']
- [pad_source_0, '0', blocks_stream_to_vector_0, '0']

metadata:
  file_format: 1
